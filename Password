import java.util.Scanner;

public class Password {
    public static void main(String[] args) {
        String email = "neymar@gmail.com";
        String password = "101010";

        System.out.print("Por favor, insira seu email : ");
        Scanner scanner1 = new Scanner(System.in);
        String pass1 = scanner1.nextLine();

        if (!email.equals(pass1)) {
            System.out.println("Email não cadastrado");
        } else {

            System.out.println("Por favor, insira sua senha");
            Scanner scanner2 = new Scanner(System.in);
            String pass2 = scanner2.nextLine();

            if (password.equals(pass2)) {
                System.out.println("Bem vindo usuário");
            } else {
                System.out.println("Senha incorreta");
            }
        }
    }
}
